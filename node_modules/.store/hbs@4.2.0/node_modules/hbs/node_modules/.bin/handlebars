#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/node/express-demo/node_modules/.store/handlebars@4.7.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/node/express-demo/node_modules/.store/handlebars@4.7.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../handlebars@4.7.7/node_modules/handlebars/bin/handlebars" "$@"
else
  exec node  "$basedir/../../../../../handlebars@4.7.7/node_modules/handlebars/bin/handlebars" "$@"
fi
